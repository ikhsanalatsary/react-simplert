{
  "name": "react-simplert",
  "version": "0.0.3",
  "description": "ReactJS Simple Alert Component (SweetAlert Inspired)",
  "main": "./build/simplert.bundle.js",
  "homepage": "https://github.com/mazipan/react-simplert#readme",
  "author": "Irfan Maulana <mazipanneh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dist": "webpack --config webpack.dist.config.js",
    "dev": "npm run build && webpack-dev-server --inline --hot --open",
    "lint": "eslint -- .",
    "format": "prettier-standard 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mazipan/react-simplert.git"
  },
  "bugs": {
    "url": "https://github.com/mazipan/react-simplert/issues"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "react alert",
    "react sweatalert",
    "react simple alert",
    "react simplert"
  ],
  "files": [
    "src/simplert.js",
    "src/simplert.scss",
    "build/simplert.bundle.js",
    "build/simplert.bundle.js.map"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-jsx"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {},
    "rules": {}
  },
  "eslintIgnore": [
    "webpack.config.js",
    "webpack.dist.config.js",
    "build/"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "es6-promise": "^4.1.1",
    "eslint": "4.9.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.2.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "3.0.1",
    "node-sass": "^4.5.3",
    "prettier-standard": "7.0.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.1"
  }
}
